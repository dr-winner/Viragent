# DFX CANISTER ENVIRONMENT VARIABLES
DFX_VERSION='0.28.0'
DFX_NETWORK='local'
CANISTER_CANDID_PATH_VIRAGENT_BACKEND='/path/to/your/project/.dfx/local/canisters/viragent_backend/viragent_backend.did'
CANISTER_ID_VIRAGENT_FRONTEND='your_frontend_canister_id'
CANISTER_ID_VIRAGENT_BACKEND='your_backend_canister_id'
CANISTER_ID='your_backend_canister_id'
CANISTER_CANDID_PATH='/path/to/your/project/.dfx/local/canisters/viragent_frontend/assetstorage.did'
# END DFX CANISTER ENVIRONMENT VARIABLES

# OpenAI API Configuration
# Get your OpenAI API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Social Media Platform API Credentials
# Twitter/X API v2 (Get from https://developer.twitter.com)
VITE_TWITTER_CLIENT_ID=your_twitter_client_id_here
VITE_TWITTER_CLIENT_SECRET=your_twitter_client_secret_here
VITE_TWITTER_REDIRECT_URI=http://localhost:5173/auth/twitter/callback
VITE_TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Instagram Basic Display API (Get from https://developers.facebook.com)
VITE_INSTAGRAM_CLIENT_ID=your_instagram_client_id_here
VITE_INSTAGRAM_CLIENT_SECRET=your_instagram_client_secret_here
VITE_INSTAGRAM_REDIRECT_URI=http://localhost:5173/auth/instagram/callback

# LinkedIn API (Get from https://developer.linkedin.com)
VITE_LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
VITE_LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here
VITE_LINKEDIN_REDIRECT_URI=http://localhost:5173/auth/linkedin/callback

# Facebook Graph API (Optional - for Facebook posts)
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here
VITE_FACEBOOK_APP_SECRET=your_facebook_app_secret_here
VITE_FACEBOOK_REDIRECT_URI=http://localhost:5173/auth/facebook/callback
